<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="EditTextHoloColorsAppTheme" parent="android:Widget.EditText">
        <item name="android:background">@drawable/holocolorsapptheme_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="AutoCompleteTextViewHoloColorsAppTheme" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/holocolorsapptheme_list_selector_holo_light</item>
        <item name="android:background">@drawable/holocolorsapptheme_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">bottom</item>
    </style>

    <style name="CheckBoxHoloColorsAppTheme" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/holocolorsapptheme_btn_check_holo_light</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="RadioButtonHoloColorsAppTheme" parent="android:Widget.CompoundButton.RadioButton" >
        <item name="android:button">@drawable/holocolorsapptheme_btn_radio_holo_light</item>
    </style>

    <style name="ButtonHoloColorsAppTheme" parent="android:Widget.Button">
        <item name="android:background">@drawable/holocolorsapptheme_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ImageButtonHoloColorsAppTheme" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/holocolorsapptheme_btn_default_holo_light</item>
    </style>

    <style name="SpinnerHoloColorsAppTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/holocolorsapptheme_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/holocolorsapptheme_list_selector_holo_light</item>
    </style>

    <style name="SpinnerDropDownItemHoloColorsAppTheme" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/holocolorsapptheme_btn_radio_holo_light</item>
    </style>
    <style name="TabHoloColorsAppTheme">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/holocolorsapptheme_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextHoloColorsAppTheme">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">bottom</item>
        <item name="android:minHeight">@dimen/gap_xhigh</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarHoloColorsAppTheme" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/holocolorsapptheme_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/holocolorsapptheme_progress_indeterminate_horizontal_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="SeekBarHoloColorsAppTheme" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/holocolorsapptheme_scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/holocolorsapptheme_scrubber_progress_horizontal_holo_light</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/holocolorsapptheme_scrubber_control_selector_holo_light</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>

    <style name="ToggleHoloColorsAppTheme" parent="android:Widget.Button.Toggle">
        <item name="android:background">@drawable/holocolorsapptheme_btn_toggle_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ListViewHoloColorsAppTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/holocolorsapptheme_list_selector_holo_light</item>
    </style>

    <style name="ListViewHoloColorsAppTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/holocolorsapptheme_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemHoloColorsAppTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="text_icon"> <!-- Used by IconView and IconTextView -->

        <item name="font">regular</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="icontextview_iconPadding">@dimen/gap_small</item>
    </style>

</resources>